{
  "name": "coolestprojects-backend",
  "version": "0.0.1",
  "description": "Coolest project services",
  "main": "index.js",
  "scripts": {
    "prestart": "sh ./start.sh",
    "start": "node index.js",
    "development": "nodemon index.js",
    "development:light": "npm i && NODE_ENV=production npx nodemon index.js",
    "debugging": "node --inspect=0.0.0.0:9229 index.js",
